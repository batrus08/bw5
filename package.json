{
  "name": "bot-backend-advanced-grid",
  "version": "1.2.0",
  "private": true,
  "description": "Express + Prisma backend for WhatsApp & Telegram bots with grid keyboard and QRIS media_id.",
  "main": "server.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "start": "prisma generate && prisma db push && node server.js",
    "start:safe": "node server.js",
    "postinstall": "prisma generate",
    "dev": "node server.js",
    "seed": "node src/db/migrate/seed.js",
    "prisma:generate": "prisma generate",
    "prisma:db-push": "prisma db push",
    "prisma:migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "express": "^4.19.2",
    "prisma": "^5.22.0",
    "@prisma/client": "^5.22.0"
  }
}
